# Que - https://www.codechef.com/problems/NOTINCOM?tab=statement

# cook your dish here
T = int(input())

for i in range(T):
    N, M = map(int, input().split())
    A = list(map(int, input().split()))
    B = list(map(int, input().split()))
    
    list1 = A + B
    
    list2 = set(list1)
    
    print(len(list1) - len(list2))
