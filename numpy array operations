import numpy as np

# Input array A
A = np.array([1, 2, 3, 4, 5])

# Compute the square of each element in array A
squared_A = np.square(A)

# Compute the sum of all elements in array A
sum_A = np.sum(A)

# Compute the mean of all elements in array A
mean_A = np.mean(A)

# Output the results
print("Array A:", A)
print("Squared A:", squared_A)
print("Sum of A:", sum_A)
print("Mean of A:", mean_A)
